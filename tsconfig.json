{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "lib": [
            "esnext.asynciterable"
        ],
        "paths": {
            "@App/*": ["src/*"],
            "*": [
                "node_modules/*",
                "src/types/*"
            ]
        }
    }
}
